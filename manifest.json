{
  "name": "All Tab Export",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["tabs", "https://*/*"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "hello.png"
  },
  "background": {
    "page": "background.html"
  }
}
